

        <%- include('./partials/scripts') -%>
        
        <body>
        
            <!-- Site Container -->
            <div id="site-container">
        
                <!-- Dashboard Area -->
                <div class="dashboard-area">
        
                    <!-- Dashboard Header -->
                    <%- include('./partials/header') -%>
                    <!-- Dashboard Header -->
        
                    <!-- Dashboard Main -->
                    <main class="dashboard-main">
                        <div class="container">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="dashboard-card full-view-card">
                                        <h1 class="dashboard-card-heading text-center">Add Bank</h1>
        
                                        <!-- Tab Navigation -->
                                        <div class="tab-navigation money-transfer-tabs marT30">
                                            
                                            <div class="tab-content">
                                                <div id="single-transaction" class="tab-pane active">
                                                <% for(var i = 0; i < errors.length; i++){ %>
                                                    <li> <%= errors[i].msg %> </li>
                                                    <% } %> 
                                                    <% if(success_msg){ %>
                                                    <%= success_msg %>
                                                    <% } %> 

                                                    <form method="post" id="upload_form" enctype="multipart/form-data" action="/upload">
                                                        <div class="form login-form">
                                                            <div class="form-group">
                                                              
                                                            <label class="form-group-label">Email address</label>
                                                            <input type="file" id="file" name="filename" class="form-control" placeholder="example@gmail.com" />
                                                            </div>
                                                         
                                                            <div class="form-group form-group-submit">
                                                            <input type="submit" id="signIn_form_submit" name="form_submit" class="primary-btn submit-btn" value="Login">
                                                            </div>
                                                        </div>
                                                      </form> 
                                                   
        
                                                </div>
                                                <div id="bulk-transaction" class="tab-pane">
                                                    <div class="form marT30">
                                                        <div class="form-group">
                                                            <label class="form-group-label">To</label>
                                                            <div class="custom-input-file">
                                                                <input type="file" id="sendToMultiple" class="inputfile" accept=".csv" />
                                                                <label for="sendToMultiple">
                                                                    <span class="selected-value">No file Selected</span>
                                                                    <strong class="file-btn"><span class="file-btn-text">Browse</span></strong>
                                                                    </label>
                                                            </div>
                                                        </div>
                                                        <div class="form-group form-group-submit text-center">
                                                            <a href="bulk-transfer-step2.html" class="primary-btn submit-btn">Upload</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Tab Navigation -->

                                        <div class="theme-table-wrapper">
                                                <table class="theme-table responsive-table marT50 marT20-xs">
                                                <thead>
                                                <tr>
                                                <th class="has-head-label from-label">Sr no</th>
                                                <th>Transaction id.</th>
                                                <th>Sending Bank</th>
                                                <th>Receiving Bank</th>
                                                <th class="has-head-label to-label">Amount</th>
                                                <th class="has-head-label to-label">Date</th>
                                                <th class="has-head-label to-label">Notes</th>
                                                
                                                </tr>
                                                </thead>
                                                <tbody>
                                                <% for(var i=0;i<output.list.length;i++){%>    
                                                <tr>
                                                <td class="has-cell-label from-label"><%=output.list[i].Sr %></td>
                                                <td><%=output.list[i].Transaction_id %></td>
                                                <td><%=output.list[i].Sending_bank_id %></td>
                                                <td><%=output.list[i].Receiving_bank_id %></td>
                                                <td class="has-cell-label to-label"><%=output.list[i].amount %></td>
                                                
                                                <td><%=output.list[i].date %></td>
                                                <td><%=output.list[i].Notes %></td>
                                                
                                             
                                                
                                                
            
                                                    
                                                </tr>
                                                <% } %>
                                                
                                                </tbody>
                                                </table>
                                                <!-- <div class="text-center marT30">
                                                <a href="bulk-transfer-step3.html" class="primary-btn">Record to Ledger</a>
                                                </div> -->
                                                </div>
        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </main>
                    <!-- Dashboard Main -->
        
        
        
                </div>
                <!-- Dashboard Area -->
        
            </div>
            <!-- Site Container -->
        
            <!-- Scripts -->
            <!-- Scripts -->
        
        </body>
        
        </html>