

        <%- include('./partials/scripts') -%>
        
        <body>
        
            <!-- Site Container -->
            <div id="site-container">
        
                <!-- Dashboard Area -->
                <div class="dashboard-area">
        
                    <!-- Dashboard Header -->
                    <%- include('./partials/header') -%>
                    <!-- Dashboard Header -->
        
                    <!-- Dashboard Main -->
                    <main class="dashboard-main">
                        <div class="container">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="dashboard-card full-view-card">
                                        <h1 class="dashboard-card-heading text-center">Edit Bank</h1>
        
                                        <!-- Tab Navigation -->
                                        <div class="tab-navigation money-transfer-tabs marT30">
                                            
                                            <div class="tab-content">
                                                <div id="single-transaction" class="tab-pane active">
                                                <% for(var i = 0; i < errors.length; i++){ %>
                                                    <li> <%= errors[i].msg %> </li>
                                                    <% } %> 
                                                    <% if(success_msg){ %>
                                                    <%= success_msg %>
                                                    <% } %> 
                                                    
                                                    <form method="post" action="editBank">
                                                        <div class="form marT30">
                                                                <div class="form-group">
                                                                        <label class="form-group-label">Account Type</label>
                                                                        <select name="type" class="form-control theme-selectbox">
                                                                        <option value="bank" <% if(bankDetail.type=='bank') { %> selected="selected" <% }%> >Bank</option>
                                                                        <option value="regulator" <% if(bankDetail.type=='regulator') { %> selected="selected" <% }%> >Regulator</option>

                                                                       
                                                                        
                                                                        </select>
                                                                    </div>
                                                        <div class="form-group">
                                                            <label class="form-group-label">Name</label>
                                                            <input type="text" name="name" class="form-control" placeholder="Bank name" value="<%=bankDetail.name%>" />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="form-group-label">Email</label>
                                                            <input type="text" name="email" class="form-control" placeholder="Enter email id" value="<%=bankDetail.email%>" />
                                                        </div>
                                                        <div class="form-group">
                                                                <label class="form-group-label">Account no</label>
                                                                <input type="text" name="account_no" class="form-control" placeholder="Enter account no" value="<%=bankDetail.account_no%>" />
                                                            </div>
                                                        <div class="form-group">
                                                            <label class="form-group-label">Address</label>
                                                            <input type="text" name="address" class="form-control" placeholder="Bank address" value="<%=bankDetail.address%>" />
                                                        </div>
                                                      <div class="form-group">
                                                            <label class="form-group-label">Telephone</label>
                                                            <input type="text" name="telephone" class="form-control" placeholder="Enter telephone number" value="<%=bankDetail.telephone%>" />
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="form-group-label">Notes</label>
                                                            <input type="text" name="notes" class="form-control" placeholder="Enter Notes" value="<%=bankDetail.notes%>" />
                                                        </div>
                                                        <div class="form-group">
                                                                <label class="form-group-label">Status</label>
                                                                <select name="status" class="form-control theme-selectbox">
                                                                <option value="1" <% if(bankDetail.status=='1'){%> selected="selected" <% } %>>Active</option>
                                                                <option value="0" <% if(bankDetail.status=='0'){%> selected="selected" <% } %>>Inactive</option>
                                                                
                                                                </select>
                                                            </div>
                                                        
                                                        <div class="form-group form-group-submit text-center">
                                                            <input type="hidden" value="<%=bankDetail._id%>" name="_id">
                                                            <input type="submit" class="primary-btn submit-btn" value="Save">
                                                        </div>
                                                    </div>
                                                </form>
        
                                                </div>
                                                <div id="bulk-transaction" class="tab-pane">
                                                    <div class="form marT30">
                                                        <div class="form-group">
                                                            <label class="form-group-label">To</label>
                                                            <div class="custom-input-file">
                                                                <input type="file" id="sendToMultiple" class="inputfile" accept=".csv" />
                                                                <label for="sendToMultiple">
                                                                    <span class="selected-value">No file Selected</span>
                                                                    <strong class="file-btn"><span class="file-btn-text">Browse</span></strong>
                                                                    </label>
                                                            </div>
                                                        </div>
                                                        <div class="form-group form-group-submit text-center">
                                                            <a href="bulk-transfer-step2.html" class="primary-btn submit-btn">Upload</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Tab Navigation -->
        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </main>
                    <!-- Dashboard Main -->
        
        
        
                </div>
                <!-- Dashboard Area -->
        
            </div>
            <!-- Site Container -->
        

        
        </body>
        
        </html>