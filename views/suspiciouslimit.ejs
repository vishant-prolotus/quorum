

        <%- include('./partials/scripts') -%>
        
        <body>
        
            <!-- Site Container -->
            <div id="site-container">
        
                <!-- Dashboard Area -->
                <div class="dashboard-area">
        
                    <!-- Dashboard Header -->
                    <%- include('./partials/header') -%>
                    <!-- Dashboard Header -->
        
                    <!-- Dashboard Main -->
                    <main class="dashboard-main">
                        <div class="container">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="dashboard-card full-view-card">
                                        <h1 class="dashboard-card-heading text-center">Add Suspicious Limit</h1>
        
                                        <!-- Tab Navigation -->
                                        <div class="tab-navigation money-transfer-tabs marT30">
                                            
                                            <div class="tab-content">
                                                <div id="single-transaction" class="tab-pane active">
                                                <% for(var i = 0; i < errors.length; i++){ %>
                                                    <li> <%= errors[i].msg %> </li>
                                                    <% } %> 
                                                    <% if(success_msg){ %>
                                                    <%= success_msg %>
                                                    <% } %> 
                                                    <form method="post" action="suspiciouslimit">
                                                        <div class="form marT30">
                                                            <div class="form-group">
                                                                <label class="form-group-label">Amount</label>
                                                                <input type="text" name="limit" class="form-control" placeholder="Enter Limit"  />
                                                            </div>

                                                            <div class="form-group form-group-submit text-center">
                                                                <input type="submit" class="primary-btn submit-btn" value="Save">
                                                            </div>
                                                    </div>
                                                </form>
        
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Tab Navigation -->
        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </main>
                    <!-- Dashboard Main -->
        
        
        
                </div>
                <!-- Dashboard Area -->
        
            </div>
            <!-- Site Container -->
        
            <!-- Scripts -->
            <!-- Scripts -->
        
        </body>
        
        </html>